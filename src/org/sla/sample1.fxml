<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.sla.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox prefHeight="13.0" prefWidth="600.0">
               <children>
                  <TextField fx:id="IPAddressText" prefHeight="27.0" prefWidth="206.0" promptText="IP Address" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="portText" promptText="Port" />
               </children>
            </HBox>
            <TextField fx:id="statusText" />
            <Button fx:id="startButton" mnemonicParsing="false" text="Start" />
            <Canvas fx:id="canvas" height="690.0" width="600.0" />
         </children>
      </VBox>
   </children>
</GridPane>
